{
  "name": "polished",
  "version": "5.0.0",
  "description": "A lightweight toolset for writing styles in Javascript.",
  "license": "MIT",
  "author": "Brian Hough <hello@brianhough.co> (https://polished.js.org)",
  "homepage": "https://polished.js.org",
  "bugs": "https://github.com/styled-components/polished/issues",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/styled-components/polished.git"
  },
  "keywords": [
    "styled-components",
    "polished",
    "emotion",
    "glamor",
    "css-in-js",
    "inline-styles",
    "react",
    "typescript",
    "color manipulate",
    "color manipulation",
    "curried color manipulation",
    "color",
    "colour"
  ],
  "main": "dist/polished.cjs.js",
  "module": "dist/polished.esm.js",
  "types": "lib/index.d.ts",
  "sideEffects": false,
  "scripts": {
    "oldbuild": "yarn build:lib && yarn build:dist",
    "build": "esbuild src/index.ts --bundle --format=esm --outfile=dist/polished.esm.js --minify --sourcemap --target=es2015",
    "prebuild:lib": "shx rm -rf lib/*",
    "build:lib": "cross-env BABEL_ENV=cjs babel --out-dir lib src --ignore test.js",
    "prebuild:umd": "shx rm -rf dist/*",
    "prebuild:dist": "shx rm -rf dist/*",
    "build:dist": "rollup -c rollup.config.js --bundleConfigAsCjs",
    "build:watch": "npm-watch",
    "test": "jest src",
    "typescript": "tsc ./typescript-test.ts --noEmit --target es6 --module es2015 --moduleResolution node --allowJs",
    "lint": "eslint src",
    "prepare": "yarn build && yarn typescript && husky install",
    "semantic-release": "semantic-release",
    "preinstall": "npx only-allow pnpm"
  },
  "lint-staged": {
    "src/**/*.js": [
      "prettier --write",
      "eslint --fix"
    ]
  },
  "watch": {
    "build:lib": "src/**/*.js"
  },
  "dependencies": {
    "@babel/runtime": "^7.21.5",
    "@types/preval.macro": "^3.0.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.21.5",
    "@babel/core": "^7.21.8",
    "@babel/eslint-parser": "^7.21.8",
    "@babel/preset-env": "^7.21.5",
    "@babel/preset-typescript": "^7.21.5",
    "@babel/runtime-corejs3": "^7.21.5",
    "@rollup/plugin-babel": "^6.0.3",
    "@rollup/plugin-node-resolve": "^15.0.2",
    "@rollup/plugin-replace": "^5.0.2",
    "@rollup/plugin-terser": "^0.4.1",
    "@types/jest": "^29.5.1",
    "@types/node": "^20.1.4",
    "babel-jest": "^29.5.0",
    "babel-plugin-add-module-exports": "^1.0.2",
    "babel-plugin-annotate-pure-calls": "^0.4.0",
    "babel-plugin-macros": "^3.1.0",
    "babel-plugin-preval": "5.1.0",
    "core-js": "3",
    "cross-env": "^7.0.3",
    "cz-conventional-changelog": "^3.1.0",
    "esbuild": "^0.17.19",
    "eslint": "^8.40.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-plugin-import": "^2.27.5",
    "husky": "^8.0.3",
    "jest": "^29.5.0",
    "jest-environment-jsdom": "^29.5.0",
    "lint-staged": "^13.2.2",
    "npm-watch": "^0.11.0",
    "prettier": "^2.8.8",
    "preval.macro": "^5.0.0",
    "ramda": "^0.29.0",
    "rollup": "^3.21.7",
    "semantic-release": "^21.0.2",
    "shx": "^0.3.4",
    "ts-jest": "^29.1.0",
    "ts-node": "^10.9.1",
    "turbo": "^1.9.4",
    "typescript": "5.0.4"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "collective": {
    "type": "opencollective",
    "url": "https://opencollective.com/polished"
  },
  "engines": {
    "node": ">= 14"
  }
}
